      ],
      "Commercetools\\Tools\\": [
        "tools"
      ]
    }
  },
  "require": {
    "php": ">=7.2",
    "ext-json": "*",
    "guzzlehttp/psr7": "^1.6",
    "guzzlehttp/guzzle": "^6.0 || ^7.0",
    "psr/cache": "^1.0",
    "psr/simple-cache": "^1.0",
    "psr/log": "^1.0",
    "psr/http-client": "^1.0",
    "psr/http-message": "^1.0",
    "cache/filesystem-adapter": "^1.0"
