{
  "name": "commercetools/spec-sdks",
  "license": "MIT",
  "type": "library",
  "description": "Lorem ipsum",
  "autoload": {
    "psr-4": {
      "Commercetools\\Api\\": [
        "lib/commercetools-api/src/"
      ],
      "Commercetools\\Import\\": [
        "lib/commercetools-import/src/"
      ],
      "Commercetools\\Ml\\": [
